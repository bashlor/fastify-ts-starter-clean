{
  "name": "api",
  "version": "1.0.0",
  "description": "api",
  "author": "Elie Samuel",
  "type": "module",
  "dependencies": {
    "@fastify/awilix": "^3.0.2",
    "@fastify/cors": "^8.3.0",
    "awilix": "^8.0.1",
    "fastify": "^4.17.0",
    "fastify-plugin": "^4.5.0",
    "lowdb": "^6.0.1",
    "neverthrow": "^6.0.0",
    "typia": "^4.1.7"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.1.2",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "cucumber-pretty": "^6.0.1",
    "eslint": "^8.41.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "ts-patch": "^3.0.2",
    "tsup": "^6.7.0",
    "typescript": "5.1.6",
    "vite": "^4.4.9",
    "vite-node": "^0.34.1",
    "vitest": "^0.34.1"
  },
  "scripts": {
    "prepare": "ts-patch install",
    "dev": "nodemon",
    "bundle": "tsup module/core/src/app.ts --format esm --out-dir bundle",
    "build": "tsc",
    "start": "node ../../pruned/build/app.js",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "features": "tsc && cucumber-js test --fail-fast",
    "unit": "vitest --config vitest.config.unit.ts",
    "unit:watch": "vitest --config vitest.config.unit.ts --watch",
    "integration": "vitest --config vitest.config.integration.ts"
  },
  "volta": {
    "node": "20.5.1"
  }
}