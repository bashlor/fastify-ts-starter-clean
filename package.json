{
  "name": "api",
  "version": "1.0.0",
  "description": "api",
  "author": "Elie Samuel",
  "type": "module",
  "dependencies": {
    "@fastify/awilix": "^3.2.0",
    "@fastify/cors": "^8.4.1",
    "awilix": "^9.0.0",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "lowdb": "^6.1.1",
    "neverthrow": "^6.1.0",
    "typia": "^5.2.4"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.6.0",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cucumber-pretty": "^6.0.1",
    "eslint": "^8.52.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "ts-patch": "^3.0.2",
    "tsup": "^6.7.0",
    "typescript": "5.1.6",
    "vite": "^4.5.0",
    "vite-node": "^0.34.6",
    "vitest": "^0.34.6"
  },
  "scripts": {
    "prepare": "ts-patch install",
    "dev": "nodemon",
    "bundle": "tsup module/core/src/app.ts --format esm --out-dir bundle",
    "build": "tsc",
    "start": "node ../../pruned/build/app.js",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "features": "tsc && cucumber-js test --fail-fast",
    "unit": "vitest --config vitest.config.unit.ts",
    "unit:watch": "vitest --config vitest.config.unit.ts --watch",
    "integration": "vitest --config vitest.config.integration.ts"
  },
  "volta": {
    "node": "20.5.1"
  }
}